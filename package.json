{
  "name": "@praburangki/web-utilities",
  "version": "1.0.6",
  "description": "Utility for web projects",
  "license": "MIT",
  "author": "praburangki<https://github.com/praburangki>",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "prepublishOnly": "pnpm run build",
    "build": "rimraf dist && tsup src/index.ts --dts --format cjs,esm",
    "test": "vitest",
    "release": "bumpp package.json --commit --push --tag && pnpm publish",
    "test:cov": "vitest --coverage"
  },
  "devDependencies": {
    "@praburangki/eslint-config": "1.0.3",
    "@types/node": "^17.0.25",
    "bumpp": "^7.1.1",
    "c8": "^7.11.2",
    "eslint": "^8.13.0",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-jest": "^26.1.4",
    "esno": "^0.14.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
